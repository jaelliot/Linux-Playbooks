{
    "policies": {
        "DisableFirefoxAccounts": true,
        "DisablePocket": true,
        "DisableTelemetry": true,
        "EnableTrackingProtection": "standard",
        "DNSOverHTTPS": {
            "Enabled": true
        },
        "PopupBlocking": {
            "Default": true
        },
        "Cookies": {
            "AcceptThirdParty": "never"
        },
        "NewTabPage": {
            "PreventUrlOverrides": true,
            "URL": "about:home"
        },
        "SearchBar": "separate",
        "Homepage": {
            "URL": "about:home",
            "Locked": true,
            "StartPage": "homepage"
        },
        "Startup": {
            "RestoreSession": true
        },
        "Tabs": {
            "OpenInForeground": false
        },
        "Bookmarks": {
            "MenuAndToolbarFolder": "Firefox defaults"
        },
        "Preferences": {
            "BackgroundAppUpdate": true,
            "UseSystemColors": false,
            "Theme": "dark",
            "Autoplay": {
                "Default": "block"
            },
            "AutoScroll": true,
            "SmoothScroll": true,
            "ShowTouchKeyboard": true,
            "UnderlineLinks": true,
            "DefaultBrowserAgentEnabled": false,
            "HttpsOnlyMode": true,
            "NetworkPrediction": false,
            "DoNotTrack": true,
            "PasswordBreaches": true,
            "BlockDangerousContent": true,
            "BlockDownloads": true,
            "WarnUnwantedSoftware": true
        },
        "FlashPlugin": {
            "Default": false,
            "Locked": true
        },
        "ExtensionSettings": {
            "*": {
                "blocked_install_message": "Custom message about why addons cannot be installed.",
                "install_sources": ["about:addons", "https://addons.mozilla.org/"],
                "installation_mode": "blocked",
                "allowed_types": ["extension"]
            }
        },
        "RequestedLocales": ["en-US"]
    }
}
